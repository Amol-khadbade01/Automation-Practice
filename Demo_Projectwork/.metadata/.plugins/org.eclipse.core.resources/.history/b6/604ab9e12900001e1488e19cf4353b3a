package Example;

public class ReverseString {

	public static void main(String[] args) 
	{
		String Frist_String="Amolkhadbade", Second_String ="";
		char ch;
		
		System.out.println("The String Before Reverse"  +  Frist_String);
		for(int i=0;i<Frist_String.length();i++)
		{
			ch=Frist_String.charAt(i);
			Second_String=ch+Second_String;
	
		}
		System.out.println("After String Reverse" + Second_String);
	}

}
